public class ExactChangeNumber {

	private static final int NUM_DENOMINATIONS = 3;
	private static final int PENCES_IN_POUND = 240;
	private static final int INF = 10000;//max can be if all denominations are 1
	
	/**
	 * 
	 * @return ignore index 0
	 */
	private static int[] getCosts() {
		int[] result = new int[PENCES_IN_POUND];
		for(int index=result.length-1;index>0;index--){
			result[index]=INF;
		}
		
		for(int index=result.length-1;index>0;index--){
			if(result[index]==INF){
				
			}
		}
		
		return result;
	}
}
